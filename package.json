{
  "name": "11-components",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "Ahmad Milzam <ahmadmilzam91@gmail.com>",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "makedir": "mkdir dist",
    "copy": "cp -R src/assets/ dist/assets/",
    "clean-copy": "npm run clean && npm run makedir && npm run copy",
    "prestart": "npm run clean-copy",
    "start": "cross-env NODE_ENV=development webpack-dev-server --content-base dist --open --hot",
    "stage": "cross-env NODE_ENV=production webpack-dev-server --content-base dist --open --hot",
    "build": "npm run clean-copy && cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "jquery": "^3.2.0",
    "vee-validate": "^2.0.0-beta.25",
    "velocity-animate": "^1.5.0",
    "vue": "^2.2.1",
    "vue-resource": "^1.3.1",
    "vue-router": "^2.4.0",
    "vuex": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-latest": "^6.0.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "csskit-npm": "^2.0.2",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.3",
    "sass-loader": "^5.0.1",
    "style-loader": "^0.14.1",
    "vue-loader": "^11.1.4",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^2.2.0",
    "webpack-clean-obsolete-chunks": "^0.1.8",
    "webpack-dev-server": "^2.2.0"
  },
  "browserslist": [
    "Android 2.3",
    "Android >= 4",
    "Chrome >= 20",
